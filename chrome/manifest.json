{
	"manifest_version": 3,
	"name": "LinkPop",
	"version": "1.0.0",
	"description": "Pop up any link.",
	"permissions": ["contextMenus"],
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"action": {
		"default_icon": {
			"16": "icons/icon-16x16.png",
			"32": "icons/icon-32x32.png"
		}
	}
}